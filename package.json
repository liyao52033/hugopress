{
  "name": "hugo-teek",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "server": "node scripts/add-frontmatter.js && hugo server --disableFastRender -D -F",
    "server:dev": "bash start.sh",
    "build": "hugo -D -F",
    "dev:prod": "hugo server --environment production --bind 0.0.0.0",
    "build:prod": "bash build.sh",
    "clean": "hugo --cleanDestinationDir",
    "proxy": "node scripts/proxy.js",
    "purgecss": "purgecss --css static/scss/*.css --content **/*.html",
    "build:js": "esbuild static/js/*.js --bundle --minify --tree-shaking=true --outdir=static/js/dist --format=esm --splitting=true",
    "build:js:theme": "node build-theme.cjs",
    "critical:gulp": "gulp critical-css",
    "critical:script": "node scripts/critical-css.js",
    "build:critical": "yarn run build && yarn run critical:script"
  },
  "keywords": [
    "hugo"
  ],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "autoprefixer": "^10.4.22",
    "esbuild": "^0.27.1",
    "glob": "^13.0.0",
    "gray-matter": "^4.0.3",
    "http-proxy": "^1.18.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1"
  }
}
