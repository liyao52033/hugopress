{
  "name": "hugo-teek",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "server": "node scripts/add-frontmatter.js && hugo server --disableFastRender -D -F",
    "build": "hugo -D -F",
    "dev": "bash start.sh",
    "build:prod": "bash build.sh",
    "clean": "hugo --cleanDestinationDir",
    "proxy": "node scripts/proxy.js",
    "purgecss": "purgecss --css static/scss/*.css --content layouts/**/*.html --output static/scss/purged/",
    "build:js": "esbuild static/js/*.js --bundle --minify --tree-shaking=true --outdir static/js/dist"
  },
  "keywords": [
    "hugo"
  ],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "gray-matter": "^4.0.3",
    "http-proxy": "^1.18.1"
  }
}